<h1>Movies#new</h1>
<p>Find me in app/views/movies/new.html.erb</p>
<%= form_for @movie, html: { multipart: true } do |f| %>
  <p><%= f.label :title %>
  <%= f.text_field :title %></p>
  <p><%= f.label :status %>
  <%= f.text_field :status %></p>
  <p><%= f.label :plot %>
  <%= f.text_field :plot %></p>
  <p><%= f.label :genre %>
  <%= f.text_field :genre %></p>
  <p><%= f.label :runtime %>
  <%= f.text_field :runtime %></p>
  <p><%= f.label :cover_image %>
  <%= f.file_field :cover_image %></p>
  <%= f.submit %>
<% end %>
